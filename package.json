  "author": "",
  "license": "ISC",
  "dependencies": {
    "@parcel/core": "^2.8.2",
    "@parcel/plugin": "^2.8.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.9",
    "husky": "^8.0.2",
    "lint-staged": "^14.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "**/*.{yml,json,md,ts,js}": [
      "prettier --write"
    ]
  },
  "engines": {
    "parcel": ">= 2.8.0"
  }
