{
  "name": "parcel-resolver-react-profiling",
  "version": "1.0.0",
  "description": "Parcel Resolver for React Profiling",
  "main": "build/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "format": "prettier --write '**/*.{yml,json,md,ts,js}'",
    "format:check": "prettier --check '**/*.{yml,json,md,ts,js}'",
